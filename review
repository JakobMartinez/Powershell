function q($file,$line) {
# return line of text from file fiven the $file. Begins with text specified $line. If no live begins with $start return $null
$content = get-content $file 
foreach ( $item in $content) {
  if ($item.strartswith($line))  {
  return $item
  }
}


function q($arr) { 
# combine $arr into a string seperated by '/' between each element and return the sring
retrun $arr -join('/')
}


function q() {
  #return proceeses sorted descendign by processName
  return Get-process | sort-object -Property name -Descending 
}  
  
